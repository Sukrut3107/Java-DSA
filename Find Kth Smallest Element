public class KthSmallest{
  public static void main(Strings[] args){
    int arr[] = {10,3,6,9,2,4,15,23}; 
    int k = 4; 
    
  } 
  static int KthSmallest(int arr[], int k){
    int temp;
    if(k>arr.length){
      return -1;
    } 
    for(int i=0; i<arr.lenght; i++){
      for(int j+1; j<arr.length; j++){
        if(arr[i]<arr[j]){
          temp = arr[i];
          arr[i] = arr[j];
          arr[j] = temp;
        }
      }
    } 
    return arr[arr.length-k];
  }
  
}
