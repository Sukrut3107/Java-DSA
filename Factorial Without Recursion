public class FiboWithoutRecr{
  public static void main(String[] argas){
    int fact=1;
    int num = 6; //ans = 720;
    //using while loop
    for(int i=num; i>0; i--){
        fact = fact*num; 
        num = num - 1;
    }
    System.out.print(fact+" ");
    
    //using while loop 
    while(num>0){
      fact = fact*num;
      num = num-1;
    }
      System.out.print(fact+" ");
  }




}
